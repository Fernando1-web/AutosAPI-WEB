@model IEnumerable<SearchResultAutoDTO.AutosDTO>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(item => item.Marca)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.Modelo)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.Year)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.Precio)
            </th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @if(Model != null){
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Marca)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modelo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Precio)
                </td>
                <td>
                    @Html.ActionLink("Modificar", "Edit", new { id = item.Id }, new { @class = "btn btn-primary" }) |
                    @Html.ActionLink("Ver", "Details", new { id = item.Id }, new { @class = "btn btn-secondary" }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
                </td>
            </tr>
        }
        }
       
    </tbody>
</table>